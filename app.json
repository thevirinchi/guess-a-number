{
	"expo": {
		"name": "guess-a-number",
		"slug": "guess-a-number",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#03a9f4"
		},
